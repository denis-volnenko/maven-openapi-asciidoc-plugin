    },
    "termsOfService" : "http://localhost:8080",
    "title" : "Swagger Maven Plugin Sample",
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "http://localhost:8080/api/"
  }, {
    "url" : "https://localhost:8080/api/"
  } ],
  "tags" : [ {
    "name" : "Дата"
  }, {
    "name" : "Задача"
  }, {
    "name" : "Проект"
  } ],
  "paths" : {
    "/api/v1/datetime/current" : {
      "get" : {
        "operationId" : "date",
        "responses" : {
          "200" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/DateDTO"
                }
              }
            },
            "description" : "successful operation"
          }
        },
        "summary" : "Получение текущей даты",
        "tags" : [ "Дата" ]
      }
    },
    "/api/v1/project/all" : {
      "get" : {
        "operationId" : "selectAllProject",
        "responses" : {
          "200" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "items" : {
                    "$ref" : "#/components/schemas/ProjectDTO"
                  },
                  "type" : "array"
                }
              }
            },
            "description" : "successful operation"
          }
        },
        "tags" : [ "Проект" ]
      }
    },
    "/api/v1/task/all" : {
      "get" : {
        "operationId" : "selectAllTask",
        "responses" : {
          "200" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "items" : {
                    "$ref" : "#/components/schemas/TaskDTO"
                  },
                  "type" : "array"
                }
              }
            },
            "description" : "successful operation"
          }
        },
        "tags" : [ "Задача" ]
      }
    }
  },
  "components" : {
    "schemas" : {
      "DateDTO" : {
        "description" : "Класс для передачи даты и времени",
        "properties" : {
          "date" : {
            "description" : "Дата/Время",
            "format" : "date-time",
            "type" : "string"
          }
        },
        "type" : "object"
      },
      "ProjectDTO" : {
        "description" : "Проект",
        "properties" : {
          "name" : {
            "description" : "Название",
            "type" : "string"
          },
          "description" : {
            "description" : "Описание",
            "type" : "string"
          },
          "version" : {
            "description" : "Версия",
            "format" : "int32",
            "type" : "integer"
          },
          "created" : {
            "description" : "Дата создания",
            "format" : "date-time",
            "type" : "string"
          },
          "updated" : {
            "description" : "Дата обновления",
            "format" : "date-time",
            "type" : "string"
          }
        },
        "type" : "object"
      },
      "TaskDTO" : {
        "description" : "Задача",
        "properties" : {
          "name" : {
            "description" : "Название",
            "type" : "string"
          },
          "description" : {
            "description" : "Описание",
            "type" : "string"
          },
          "version" : {
            "description" : "Версия",
            "format" : "int32",
            "type" : "integer"
          },
          "created" : {
            "description" : "Дата создания",
            "format" : "date-time",
            "type" : "string"
          },
          "updated" : {
            "description" : "Дата обновления",
            "format" : "date-time",
            "type" : "string"
          }
        },
        "type" : "object"
      }
    },
    "securitySchemes" : {
      "basicAuth" : {
        "scheme" : "basic",
        "type" : "http"
      }
    }
  },
  "x-original-swagger-version" : "2.0"
}